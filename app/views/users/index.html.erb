<div class="username">
<div class="contents">
<% if user_signed_in? && @user == current_user %>
  <h1>マイページ</h1>
<% else %>
  <h1>ユーザーページ</h1>
<% end %>

<h1><%= current_user.schoolname %></h1>

<h1>全ユーザー</h1>
 
  <% @users.each do |user| %>
      <p><%= link_to user.schoolname, user %></p>
  <% end %>
</div>
</div>